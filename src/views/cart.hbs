{{> header}}

<h1>Cart</h1>
<ul>
  {{#each cart.products}}
  <li>
    <h2>{{this.productId.name}}</h2>
    <p>Price: {{this.productId.price}}</p>
    <p>Category: {{this.productId.category}}</p>
    <p>Quantity: {{this.quantity}}</p>
    <button>Remove</button>
  </li>
  {{/each}}
</ul>

{{> footer}}